{
  "name": "avtovyshka-spb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc",
    "cy:run": "cypress run",
    "prepare": "husky install",
    "test": "start-server-and-test start http://localhost:3000 cy:run"
  },
  "dependencies": {
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.11",
    "cypress": "^7.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "prettier": "2.3.1",
    "start-server-and-test": "^1.12.5",
    "typescript": "4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": "prettier --write"
  }
}
